<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

<div class="ndl-holdings">
  <h3 class="ndl-debug">Debug ID: <%= @tab.record_id %></h3>

  <%= render partial: 'institution', locals: { institution: @tab.vid, holdings: @tab.institution_holdings(@tab.vid), record: @tab.record.data } %>
  <% @tab.other_institution_codes.each do |institution_code| %>
    <%= render partial: 'institution', locals: { institution: institution_code, holdings: @tab.institution_holdings(institution_code), record: @tab.record.data } %>
  <% end %>
</div>
